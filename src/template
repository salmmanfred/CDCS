province = Province:create{ ref_name = "n2", color = colour }
province.name = _("n2")
Province:register(province)
Province:add_pop(province, artisan, pop_name, relg, 5000, 0.9)
Province:add_pop(province, farmer, pop_name, relg, 40000, 0.8)
Province:add_pop(province, soldier, pop_name, relg, 8000, 0.8)
Province:add_pop(province, craftsmen, pop_name, relg, 5000, 0.9)
Province:add_pop(province, bureaucrat, pop_name, relg, 5000, 0.9)
Province:add_pop(province, aristocrat, pop_name, relg, 5000, 0.9)
Province:add_pop(province, clergymen, pop_name, relg, 5000, 0.9)
Province:add_pop(province, laborer, pop_name, relg, 50000, 0.8)
Province:add_pop(province, entrepreneur, pop_name, relg, 800, 0.9)
Province:add_nucleus(province, province_name)
Province:add_nucleus(province, pop_nation)